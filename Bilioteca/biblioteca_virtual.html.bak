      <script>

        window.onload=function()
        {
            ocultarTodosLosDiv();
            aparecerDiv(document.getElementById("div1"));
        }  

        function ocultarDiv(x)
        {
            x.style.display = "none";
        }

        function aparecerDiv(x)
        {
            x.style.display = "block";
        }

        function abrirDiv(num)
        {
            ocultarTodosLosDiv();

            switch(num)
            {
                case 1: aparecerDiv(document.getElementById("div1"));
                        break;

                case 2: aparecerDiv(document.getElementById("div2"));
                        ocultarTodosBdContenedores();
                        aparecerDiv(document.getElementById("bd_contenedor1"));
                        break;

                case 3: aparecerDiv(document.getElementById("div3"));
                        break;

                case 4: aparecerDiv(document.getElementById("div4"));
                        break;
            }
        }

        function abrirBdContenedor(num)
        {
            ocultarTodosBdContenedores();

            switch(num)
            {
                case 0: break;
                case 1: aparecerDiv(document.getElementById("bd_contenedor1")); break;
                case 2: aparecerDiv(document.getElementById("bd_contenedor2"));break;
                case 3: aparecerDiv(document.getElementById("bd_contenedor3"));break;
                case 4: break;
            }
        }

        function ocultarTodosLosDiv()
        {
            for(i=1; i<=2; i++)
            {
                ocultarDiv(document.getElementById("div"+i));
            }
        }

        function ocultarTodosBdContenedores()
        {       
            for(i=1; i<=3; i++)
            {
                ocultarDiv(document.getElementById("bd_contenedor"+i));
            }
        }

